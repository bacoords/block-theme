{
	"border": {
		"radius": "var(--wp--custom--button--border--radius)",
		"color": "var(--wp--custom--button--border--color)",
		"width": "var(--wp--custom--button--border--width)"
	},
	"spacing": {
		"padding": {
			"top": "var(--wp--custom--button--spacing--padding--top)",
			"right": "var(--wp--custom--button--spacing--padding--right)",
			"bottom": "var(--wp--custom--button--spacing--padding--bottom)",
			"left": "var(--wp--custom--button--spacing--padding--left)"
		}
	},
	"color": {
		"background": "var(--wp--custom--button--color--background)",
		"text": "var(--wp--custom--button--color--text)"
	},
	":hover": {
		"color": {
			"background": "var(--wp--custom--button--hover--color--background)",
			"text": "var(--wp--custom--button--hover--color--text)"
		}
	},
	":focus": {
		"color": {
			"background": "var(--wp--custom--button--focus--color--background)",
			"text": "var(--wp--custom--button--focus--color--text)"
		},
		"border": {
			"color": "var(--wp--custom--border--color)"
		}
	},
	":active": {
		"color": {
			"background": "var(--wp--custom--button--active--color--background)",
			"text": "var(--wp--custom--button--active--color--text)"
		}
	},
	":visited": {
		"color": {
			"text": "var(--wp--custom--button--visited--color--text)"
		}
	}
}
