{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "settings": {
    "appearanceTools": true,
    "color": {
      "custom": false,
      "customDuotone": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "duotone": [],
      "palette": [
        {
          "name": "Background",
          "slug": "background",
          "color": "#FEFEFE"
        },
        {
          "name": "Foreground",
          "slug": "foreground",
          "color": "#181A1F"
        },
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#3858e9"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#7b90ff"
        }
      ]
    },
    "custom": {
      "blockGap": "1.5rem"
    },
    "layout": {
      "contentSize": "650px",
      "wideSize": "1200px"
    },
    "spacing": {
      "spacingSizes": [
        {
          "size": "clamp(0.56rem, calc(0.54rem + 0.11vw), 0.63rem)",
          "slug": "20",
          "name": "2X-Small"
        },
        {
          "size": "clamp(0.88rem, 0.85rem + 0.11vi, 0.94rem)",
          "slug": "30",
          "name": "X-Small"
        },
        {
          "size": "clamp(1.13rem, calc(1.08rem + 0.22vw), 1.25rem)",
          "slug": "40",
          "name": "Small"
        },
        {
          "size": "clamp(1.69rem, calc(1.62rem + 0.33vw), 1.88rem)",
          "slug": "50",
          "name": "Medium"
        },
        {
          "size": "clamp(2.25rem, calc(2.16rem + 0.43vw), 2.50rem)",
          "slug": "60",
          "name": "Large"
        },
        {
          "size": "clamp(3.38rem, calc(3.24rem + 0.65vw), 3.75rem)",
          "slug": "70",
          "name": "X-Large"
        },
        {
          "size": "clamp(4.5rem, 4.33rem + 0.87vi, 5rem)",
          "slug": "80",
          "name": "2X-Large"
        }
      ]
    },
    "typography": {
      "customFontSize": false,
      "dropCap": false,
      "fluid": true,
      "fontFamilies": [
        {
          "fontFace": [
            {
              "fontFamily": "EBGaramond",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": [
                "file:./fonts/EBGaramond-Regular.ttf"
              ]
            },
            {
              "fontFamily": "EBGaramond",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": [
                "file:./fonts/EBGaramond-Bold.ttf"
              ]
            },
			{
			  "fontFamily": "EBGaramond",
              "fontStyle": "italic",
              "fontWeight": "400",
              "src": [
                "file:./fonts/EBGaramond-Italic.ttf"
              ]
            },
			{
              "fontFamily": "EBGaramond",
              "fontStyle": "italic",
              "fontWeight": "700",
              "src": [
                "file:./fonts/EBGaramond-BoldItalic.ttf"
              ]
			}
          ],
          "fontFamily": "\"EBGaramond\", sans-serif",
          "name": "EB Garamond",
          "slug": "ebgaramond"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": [
                "file:./fonts/Inter-Regular.ttf"
              ]
            },
            {
              "fontFamily": "Inter",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": [
                "file:./fonts/Inter-Bold.ttf"
              ]
            }
          ],
          "fontFamily": "\"Inter\", serif",
          "name": "Inter",
          "slug": "inter"
        }
      ],
      "fontSizes": [
        {
          "fluid": {
            "min": "0.94rem",
            "max": "1rem"
          },
          "size": "1rem",
          "slug": "sm",
          "name": "Small"
        },
        {
          "fluid": {
            "min": "1.13rem",
            "max": "1.25rem"
          },
          "size": "1.25rem",
          "slug": "paragraph",
          "name": "Paragraph"
        },
        {
          "fluid": {
            "min": "1.35rem",
            "max": "1.56rem"
          },
          "size": "1.56rem",
          "slug": "h6",
          "name": "H6"
        },
        {
          "fluid": {
            "min": "1.62rem",
            "max": "1.95rem"
          },
          "size": "1.95rem",
          "slug": "h5",
          "name": "H5"
        },
        {
          "fluid": {
            "min": "1.94rem",
            "max": "2.44rem"
          },
          "size": "2.44rem",
          "slug": "h4",
          "name": "H4"
        },
        {
          "fluid": {
            "min": "2.33rem",
            "max": "3.05rem"
          },
          "size": "3.05rem",
          "slug": "h3",
          "name": "H3"
        },
        {
          "fluid": {
            "min": "2.8rem",
            "max": "3.82rem"
          },
          "size": "3.82rem",
          "slug": "h2",
          "name": "H2"
        },
        {
          "fluid": {
            "min": "3.36rem",
            "max": "4.77rem"
          },
          "size": "4.77rem",
          "slug": "large",
          "name": "Large"
        },
        {
          "fluid": {
            "min": "4.03rem",
            "max": "5.96rem"
          },
          "size": "5.96rem",
          "slug": "h1",
          "name": "H1"
        }
      ]
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "color": {
      "background": "var:preset|color|background",
      "text": "var:preset|color|foreground"
    },
    "elements": {
      "button": {
        "border": {
          "radius": "0px",
          "color": "var:preset|color|background",
          "width": "2px"
        },
        "spacing": {
          "padding": {
            "top": "var:preset|spacing|20",
            "right": "var:preset|spacing|50",
            "bottom": "var:preset|spacing|20",
            "left": "var:preset|spacing|50"
          }
        },
        "color": {
          "background": "var:preset|color|primary",
          "text": "var:preset|color|background"
        },
        ":hover": {
          "color": {
			"background": "var:preset|color|secondary",
			"text": "var:preset|color|background"
          }
        },
        ":focus": {
          "color": {
			"background": "var:preset|color|secondary",
			"text": "var:preset|color|background"
          },
          "border": {
            "color": "var:preset|color|background"
          }
        },
        ":active": {
          "color": {
            "background": "var:preset|color|background",
            "text": "var:preset|color|secondary"
          }
        },
        ":visited": {
          "color": {
            "text": "var:preset|color|background"
          }
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var:preset|font-size|h-1"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var:preset|font-size|h-2"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var:preset|font-size|h-3"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var:preset|font-size|h-4"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var:preset|font-size|h-5"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var:preset|font-size|h-6"
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var:preset|font-family|ebgaramond",
          "fontWeight": "400",
          "lineHeight": "1.2"
        }
      },
      "link": {
        ":hover": {
          "color": {
            "text": "var:preset|color|foreground"
          },
          "typography": {
            "textDecoration": "underline var(--wp--preset--color--foreground)"
          }
        },
        ":focus": {
          "typography": {
            "textDecoration": "underline dashed"
          }
        },
        ":active": {
          "color": {
            "text": "var:preset|color|foreground"
          },
          "typography": {
            "textDecoration": "none"
          }
        },
        "color": {
          "text": "var:preset|color|primary"
        },
        "typography": {
          "textDecoration": "underline var(--wp--preset--color--primary)"
        }
      }
    },
    "spacing": {
      "blockGap": "var:custom|block-gap",
      "padding": {
        "top": "var:preset|spacing|30",
        "right": "var:preset|spacing|20",
        "bottom": "var:preset|spacing|30",
        "left": "var:preset|spacing|20"
      }
    },
    "typography": {
      "fontFamily": "var:preset|font-family|inter",
      "fontSize": "var:preset|font-size|paragraph",
      "lineHeight": "1.6"
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer Credit Area",
      "area": "footer"
    },
    {
      "name": "comments",
      "title": "Comments",
      "area": "uncategorized"
    },
    {
      "name": "post-meta",
      "title": "Post Meta",
      "area": "uncategorized"
    }
  ],
  "version": 2
}
